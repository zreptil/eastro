{
  "name": "eastro",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "i18n-01-extract": "ng extract-i18n app --output-path src/locale --out-file messages.en-GB.xliff --format xliff --progress",
    "i18n-02-upload-english": "start https://crowdin.com/project/eastro/content/files",
    "i18n-12-extract-and-upload-english": "npm run i18n-01-extract && npm run i18n-02-upload-english",
    "i18n-03-create-messages": "tsc tools/i18n-create-messages.ts && node tools/i18n-create-messages.js",
    "i18n": "npm run i18n-01-extract && npm run i18n-03-create-messages",
    "ng": "ng",
    "start": "ng serve --port 3203 --open",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/material": "^19.2.2",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/router": "^19.2.1",
    "base64-arraybuffer": "^1.0.2",
    "extract-zip": "^2.0.1",
    "fast-sha256": "^1.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "~19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@angular/localize": "^19.2.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.8.2",
    "xliff": "^6.1.0"
  }
}
